{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "cd .. && husky install client/.husky",
    "check-lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "check-format": "prettier --check .",
    "format": "prettier --write **/*.* **/*.jsx",
    "test-all": "pnpm run check-format && pnpm run check-lint && pnpm run build",
    "commit": "cz"
  },
  "husky": {
    "hooks": {
      "commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji",
      "format": "[emoji] [name]: [subject]"
    }
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.9.5",
    "axios": "1.5.0",
    "js-cookie": "3.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.1.2",
    "react-router-dom": "6.16.0"
  },
  "devDependencies": {
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "4.0.3",
    "commitlint": "17.7.1",
    "commitlint-config-gitmoji": "2.3.1",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "vite": "4.4.5"
  }
}
